<?xml version="1.0" encoding="utf-8"?>
<navigation android:id="@+id/nav_graph" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
  app:startDestination="@id/FirstFragment">

  <fragment
    android:id="@+id/FirstFragment" android:name="com.lb.common_utils.sample.FirstFragment" android:label="@string/first_fragment_label" tools:layout="@layout/fragment_first">

    <action
      android:id="@+id/action_FirstFragment_to_SecondFragment" app:destination="@id/SecondFragment" />
  </fragment>
  <fragment
    android:id="@+id/SecondFragment" android:name="com.lb.common_utils.sample.SecondFragment" android:label="@string/second_fragment_label" tools:layout="@layout/fragment_second">

    <action
      android:id="@+id/action_SecondFragment_to_FirstFragment" app:destination="@id/FirstFragment" />
  </fragment>
</navigation>
